    "num: int = int(input(\"Insert a number: \"))\n",
    "\n",
    "if num > 1:\n",
    "    for i in range(2, int(num ** 0.5 + 1)):\n",
    "        if num % i == 0:\n",
    "            print(f\"{num} is not a prime\")\n",
    "        else:\n",
    "            print(f\"{num} is a prime\")\n",
    "else:\n",
    "    print(f\"{num} is not a prime\")\n"
   ]
